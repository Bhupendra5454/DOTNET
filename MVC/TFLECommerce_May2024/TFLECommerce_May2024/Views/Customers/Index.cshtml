@{
}

@{
    List<Customer> customers = (List<Customer>)ViewData["allCustomers"];
    <ul>
        @foreach(Customer cus in customers)
        {
            <li><a href="/customers/details/@cus.Id">@cus.Name</a></li>
        }
    </ul>
}
